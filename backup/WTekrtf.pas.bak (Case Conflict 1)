unit WTekRTF;

{$mode objfpc}{$H+}

interface

uses
  Classes, SysUtils, LResources, Forms, Controls, Graphics, Dialogs;

type

  { TComponent1 }

  TWTekRTF = class(TComponent)
  private
    FInputFile:string;
     { Private declarations }
  protected
    { Protected declarations }
  public
    { Public declarations }
  published
     property InputFile read FInputFile Write FInputFile;
    { Published declarations }
  end;

procedure Register;

implementation

procedure Register;
begin
  {$I WTekRTF_icon.lrs}
  RegisterComponents('Trita',[TWTekRTF]);
end;

end.
